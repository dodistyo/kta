{
   "name": "kta-desktop",
   "description": "Desktop app KTA PKS.",
   "productName": "KTA PKS",
   "private": true,
   "version": "0.0.1",
   "homepage": "https://github.com/IT-PKS/kta-desktop",
   "main": "dist/main.js",
   "author": {
      "name": "Partai Keadilan Sejahtera (PKS)",
      "email": "humas@pks.id"
   },
   "devEngines": {
      "node": ">=7.x",
      "npm": ">=4.x",
      "yarn": ">=1.0.0"
   },
   "scripts": {
      "postinstall": "node task-runner.js postinstall && electron-rebuild -f -w sqlite3",
      "release": "./crossbuild.sh",
      "clean-release": "node task-runner.js clean-release",
      "dev": "cross-env NODE_ENV=development PORT=9065 node task-runner.js dev",
      "build-win": "cross-env NODE_ENV=production node task-runner.js build-win",
      "build-mac": "cross-env NODE_ENV=production node task-runner.js build-mac",
      "start": "ts-node src/index.ts"
   },
   "build": {
      "productName": "KTA PKS",
      "appId": "com.pks.kta",
      "copyright": "Copyright 2020 PKS",
      "directories": {
         "app": "app",
         "buildResources": "resources",
         "output": "release"
      },
      "publish": [
         {
            "provider": "github",
            "repo": "kta-desktop"
         }
      ],
      "files": [
         "dist",
         "!dist/dll",
         "node_modules",
         "package.json"
      ],
      "dmg": {
         "contents": [
            {
               "x": 130,
               "y": 220
            },
            {
               "x": 410,
               "y": 220,
               "type": "link",
               "path": "/Applications"
            }
         ]
      },
      "win": {
         "target": "nsis",
         "publish": [
            "github"
         ]
      },
      "linux": {
         "publish": [
            "github"
         ],
         "target": [
            "deb",
            "AppImage"
         ]
      }
   },
   "dependencies": {
      "@emotion/core": "^10.0.27",
      "@emotion/styled": "^10.0.27",
      "@fortawesome/fontawesome-svg-core": "^1.2.30",
      "@fortawesome/free-brands-svg-icons": "^5.14.0",
      "@fortawesome/free-regular-svg-icons": "^5.14.0",
      "@fortawesome/free-solid-svg-icons": "^5.14.0",
      "@fortawesome/react-fontawesome": "^0.1.11",
      "@popperjs/core": "^2.4.4",
      "@types/electron-devtools-installer": "^2.2.0",
      "axios": "^0.20.0",
      "classnames": "^2.2.6",
      "electron": "8.5.0",
      "electron-log": "^4.3.0",
      "electron-rebuild": "^2.0.2",
      "electron-updater": "^4.3.5",
      "emotion-theming": "^10.0.27",
      "facepaint": "^1.2.1",
      "immer": "^7.0.9",
      "immutability-helper": "^3.1.1",
      "isomorphic-dompurify": "^0.8.0",
      "js-sha256": "^0.9.0",
      "lodash": "^4.17.20",
      "mysql": "^2.14.1",
      "node-machine-id": "^1.1.12",
      "polished": "^3.6.5",
      "qrcode": "^1.4.4",
      "rc-table": "^7.10.2",
      "react": "^16.13.1",
      "react-dom": "^16.13.1",
      "react-hook-form": "^6.8.4",
      "react-popper": "^2.2.3",
      "react-query": "^2.23.1",
      "react-redux": "^7.2.1",
      "react-router-dom": "^5.2.0",
      "react-select": "^3.1.0",
      "react-text-mask": "^5.4.3",
      "react-transition-group": "^4.4.1",
      "redux": "^4.0.5",
      "redux-form": "^8.3.6",
      "redux-thunk": "^2.3.0",
      "reflect-metadata": "^0.1.10",
      "sqlite3": "^5.0.0",
      "text-mask-addons": "^3.8.0",
      "typeorm": "0.2.26"
   },
   "devDependencies": {
      "@babel/core": "^7.11.1",
      "@babel/plugin-proposal-class-properties": "^7.10.4",
      "@babel/plugin-proposal-decorators": "^7.10.5",
      "@babel/preset-env": "^7.11.0",
      "@babel/preset-react": "^7.10.4",
      "@babel/preset-typescript": "^7.10.4",
      "@hot-loader/react-dom": "^16.13.0",
      "@types/classnames": "^2.2.10",
      "@types/facepaint": "^1.2.1",
      "@types/lodash": "^4.14.161",
      "@types/node": "^14.14.6",
      "@types/qrcode": "^1.3.5",
      "@types/react": "^16.9.46",
      "@types/react-dom": "^16.9.8",
      "@types/react-redux": "^7.1.9",
      "@types/react-router-dom": "^5.1.5",
      "@types/react-select": "^3.0.20",
      "@types/react-text-mask": "^5.4.6",
      "@types/redux-form": "^8.2.7",
      "@types/sqlite3": "^3.1.6",
      "@typescript-eslint/eslint-plugin": "^3.9.1",
      "@typescript-eslint/parser": "^3.9.1",
      "babel-loader": "^8.1.0",
      "core-js": "^3.2.1",
      "cross-env": "^7.0.2",
      "del": "^5.1.0",
      "electron-builder": "^22.8.1",
      "electron-devtools-installer": "^3.1.1",
      "eslint": "^7.7.0",
      "eslint-config-prettier": "^6.5.0",
      "eslint-plugin-prettier": "^3.1.1",
      "eslint-plugin-react": "^7.16.0",
      "file-loader": "^6.0.0",
      "mkdirp": "^1.0.4",
      "prettier": "^2.0.5",
      "react-hot-loader": "^4.12.21",
      "react-query-devtools": "^2.6.0",
      "regenerator-runtime": "^0.13.3",
      "ts-node": "3.3.0",
      "tsconfig-paths-webpack-plugin": "^3.2.0",
      "typescript": "3.3.3333",
      "url-loader": "^4.1.0",
      "webpack": "^4.44.1",
      "webpack-cli": "^3.3.12",
      "webpack-dev-server": "^3.11.0",
      "webpack-merge": "^4.2.2",
      "webpack-node-externals": "^2.5.1"
   }
}
